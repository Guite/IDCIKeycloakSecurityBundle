{
    "name": "idci/keycloak-security-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily handle you application security thanks to keycloak.",
    "license": "CECILL-C",
    "keywords": [],
    "authors": [
        {
            "name": "Gabriel BONDAZ",
            "email": "gabriel.bondaz@idci-consulting.fr"
        },
        {
            "name": "Benjamin BOUDIER",
            "email": "benjamin.boudier@idci-consulting.fr"
        }
    ],
    "require": {
        "symfony/dependency-injection": "5.*",
        "symfony/framework-bundle": "5.*",
        "symfony/routing": "5.*",
        "symfony/security-bundle": "5.*",
        "symfony/http-foundation": "5.*",
        "knpuniversity/oauth2-client-bundle": "2.*",
        "guzzlehttp/guzzle": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/expression-language": "5.*",
        "symfony/finder": "5.*",
        "symfony/form": "5.*",
        "symfony/stopwatch": "5.*",
        "symfony/twig-bundle": "5.*",
        "symfony/validator": "5.*",
        "symfony/yaml": "5.*"
    },
    "autoload": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\Tests\\": "Tests"
        }
    }
}
