{
    "name": "idci/keycloak-security-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to easily handle you application security thanks to keycloak.",
    "license": "CECILL-C",
    "keywords": [],
    "authors": [
        {
            "name": "Gabriel BONDAZ",
            "email": "gabriel.bondaz@idci-consulting.fr"
        },
        {
            "name": "Benjamin BOUDIER",
            "email": "benjamin.boudier@idci-consulting.fr"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^5|^6",
        "symfony/framework-bundle": "^5|^6",
        "symfony/routing": "^5|^6",
        "symfony/security-bundle": "^5|^6",
        "symfony/http-foundation": "^5|^6",
        "knpuniversity/oauth2-client-bundle": "^2",
        "guzzlehttp/guzzle": "7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "symfony/expression-language": "^5|^6",
        "symfony/finder": "^5|^6",
        "symfony/form": "^5|^6",
        "symfony/stopwatch": "^5|^6",
        "symfony/twig-bundle": "^5|^6",
        "symfony/validator": "^5|^6",
        "symfony/yaml": "^5|^6"
    },
    "autoload": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IDCI\\Bundle\\KeycloakSecurityBundle\\Tests\\": "Tests"
        }
    }
}
